{
  "name": "jwt-decoder",
  "version": "1.0.0",
  "description": "A simple JWT decoder",
  "main": "index.js",
  "scripts": {
    "build:js": "webpack --mode=production",
    "watch:js": "onchange \"src/\" -- webpack --mode=development",
    "build:html": "posthtml -c posthtml.json",
    "watch:html": "onchange \"src/views\" -- npm run build:html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fuele/jwt-decoder.git"
  },
  "keywords": [
    "JWT"
  ],
  "author": "Donovan Hubbard",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/fuele/jwt-decoder/issues"
  },
  "homepage": "https://github.com/fuele/jwt-decoder#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.18.10",
    "babel-loader": "^8.2.5",
    "eslint": "^8.22.0",
    "eslint-webpack-plugin": "^3.2.0",
    "htmlnano": "^2.0.2",
    "posthtml": "^0.16.6",
    "posthtml-cli": "^0.10.0",
    "posthtml-modules": "^0.9.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "dompurify": "^2.3.10"
  }
}
